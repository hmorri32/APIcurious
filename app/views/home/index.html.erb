<div class="container">
  <div class='login-container'>
    <% if current_user %>
      <h2><span>Welcome</span> <%= current_user.username %></h2>
      <%= link_to "Logout", logout_path, method: :delete %>
    <% else %>
      <!-- <%= image_tag("GitHub-Mark-Light-120px-plus.png") %> -->
      <%= link_to image_tag("GitHub_Logo_White.png"), login_path %>
      <%= link_to "Login", login_path %>
    <% end %>
  </div>

</div>